{
  "name": "redux-cheatsheet",
  "version": "1.0.0",
  "description": "Experimental POCs using Redux",
  "main": "index.js",
  "repository": "https://github.com/niennte/redux-cheatsheet.git",
  "author": "Irin",
  "license": "MIT",
  "browserslist": [
    "> 1%"
  ],
  "scripts": {
    "start": "yarn dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress --mode=development",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "yarn lint && jest --coverage",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "cross-env NODE_ENV=production pm2 start lib/server && pm2 logs",
    "prod:stop": "pm2 delete server"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.3",
    "deep-freeze": "^0.0.1",
    "express": "^4.16.3",
    "immutable": "4.0.0-rc.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1",
    "webpack-cli": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "7",
    "babel-plugin-flow-react-proptypes": "^24.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-flowtype": "^2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "fetch-mock": "^7.0.0-alpha.6",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.4",
    "pm2": "^3.1.2",
    "redux-mock-store": "^1.5.3",
    "rimraf": "^2.6.2",
    "webpack": "^4.19.0",
    "webpack-dev-server": "^3.1.8"
  }
}
